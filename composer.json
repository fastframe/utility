{
  "name": "fastframe/utility",
  "description": "FastFrame Utility classes",
  "license": "MIT",
  "keywords": [
    "FastFrame",
    "Utility",
    "Array Helper",
    "String Helper",
    "Priority List"
  ],
  "authors": [
    {
      "name": "David Lundgren",
      "email": "dlundgren@syberisle.net"
    }
  ],
  "autoload" : {
    "psr-4" : {
      "FastFrame\\Utility\\" : "src"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "FastFrame\\Utility\\" : "tests/src"
    }
  },
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "replace": {
    "fastframe/utility-array": "*"
  },
  "scripts" : {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-clover build/logs/clover.xml"
  }
}
